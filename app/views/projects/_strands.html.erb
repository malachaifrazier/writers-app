<% @strands.each do |strand| %>
  <%= render partial: "modals/strand_modal", locals: { strand: strand, project: @project } %>

  <div class="panel panel-default" data-id="<%= strand.id %>">
    <div class="panel-body">
      <%= link_to "#", class: "modals", data: { :"toggle" => "modal", :"target" => ".strandModal-#{strand.id}" } do %>
        <b><%= strand.name %></b>
        <%= link_to strand, method: :delete, class: "pull-right", data: { confirm: 'Are you sure?' } do %>
          <i class="fa fa-trash"></i>
        <% end %>
      <% end %>
    </div>
  </div>
<% end %>